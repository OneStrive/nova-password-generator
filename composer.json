{
    "name": "outofoffice/password-generator",
    "description": "Random Password Generation Field for Laravel Nova",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "OutOfOffice\\PasswordGenerator\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "OutOfOffice\\PasswordGenerator\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/nova": "^5.7"
    }
}
